<Style name="text-natural-lines">
	<Rule>
		&maxscale_zoom11;
		&minscale_zoom12;
		<Filter>[natural] = 'ridge' or [natural] = 'valley' or [natural] = 'gorge'</Filter>
		<TextSymbolizer face-name="DejaVu Sans Oblique" size="9" fill="black" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="line" spacing="500" allow-overlap="true" character-spacing="10">[name]</TextSymbolizer>
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>[natural] = 'ridge' or [natural] = 'valley' or [natural] = 'gorge'</Filter>
		<TextSymbolizer face-name="DejaVu Sans Oblique" size="30" fill="black" halo-radius="2" halo-fill="rgba(255,255,255,0.8)" placement="line" spacing="500" allow-overlap="true" character-spacing="60">[name]</TextSymbolizer>
	</Rule>
</Style>
