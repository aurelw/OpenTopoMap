<Style name="waterway-lines">

<!-- important rivers -->
	<Rule>
		&maxscale_zoom7;
		&minscale_zoom7;
		<Filter>([waterway] = 'river') and (([CEMT] = 'IV') or ([CEMT] = 'Va') or ([CEMT] = 'Vb') or ([CEMT] = 'VIa') or ([CEMT] = 'VIb') or ([CEMT] = 'VIb') or ([CEMT] = 'VII') or ([motorboat] = 'yes'))</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="1.1" stroke-opacity="0.6" />
	</Rule>
	<Rule>
		&maxscale_zoom8;
		&minscale_zoom10;
		<Filter>([waterway] = 'river') and (([CEMT] = 'IV') or ([CEMT] = 'Va') or ([CEMT] = 'Vb') or ([CEMT] = 'VIa') or ([CEMT] = 'VIb') or ([CEMT] = 'VIb') or ([CEMT] = 'VII') or ([motorboat] = 'yes'))</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="2" stroke-opacity="0.8" />
	</Rule>
	
<!-- less important rivers -->
	<Rule>
		&maxscale_zoom8;
		&minscale_zoom10;
		<Filter>([waterway] = 'river') and not (([CEMT] = 'IV') or ([CEMT] = 'Va') or ([CEMT] = 'Vb') or ([CEMT] = 'VIa') or ([CEMT] = 'VIb') or ([CEMT] = 'VIb') or ([CEMT] = 'VII') or ([motorboat] = 'yes'))</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="1.1" stroke-opacity="0.5" />
	</Rule>
	
	<Rule>
		&maxscale_zoom11;
		&minscale_zoom12;
		<Filter>([waterway] = 'river') and ([intermittent] != 'yes')</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="1.5" />
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom15;
		<Filter>([waterway] = 'river') and ([intermittent] != 'yes')</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="6" />
	</Rule>
	<Rule>
		&maxscale_zoom16;
		&minscale_zoom16;
		<Filter>([waterway] = 'river') and ([intermittent] != 'yes')</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="4" />
		<LineSymbolizer stroke="#a9ece9" stroke-width="3" />
	</Rule>
	
	<Rule>
		&maxscale_zoom11;
		&minscale_zoom12;
		<Filter>([waterway] = 'river') and ([intermittent] = 'yes')</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="1.5" stroke-dasharray="1.5,2" />
	</Rule>
	
<!-- streams and drains -->
	<!-- always water (intermittent!=yes) -->
	<Rule>
		&maxscale_zoom11;
		&minscale_zoom12;
		<Filter>([waterway] = 'stream') and ([tunnel] != 'yes') and ([intermittent] != 'yes')</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="0.5" />
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>([waterway] = 'stream') and ([tunnel] != 'yes') and ([intermittent] != 'yes')</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="1.5" />
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>([waterway] = 'stream') and ([tunnel] = 'yes') and ([intermittent] != 'yes')</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="1.5" stroke-dasharray="5,5" />
	</Rule>
	
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>([waterway] = 'ditch' or [waterway] = 'drain') and ([tunnel] != 'yes') and ([intermittent] != 'yes')</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="1.5" />
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>([waterway] = 'ditch' or [waterway] = 'drain') and ([tunnel] = 'yes') and ([intermittent] != 'yes')</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="1.5" stroke-dasharray="5,5" />
	</Rule>
	
	<!-- sometimes water (intermittent=yes) -->
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>([waterway] = 'stream') and ([intermittent] = 'yes')</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="4" stroke-dasharray="2,3" />
	</Rule>
	<Rule>
		&maxscale_zoom13;
		&minscale_zoom16;
		<Filter>([waterway] = 'ditch' or [waterway] = 'drain') and ([intermittent] = 'yes')</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="2.5" stroke-dasharray="3,4" />
	</Rule>
	
<!-- important canals -->
	<Rule>
		&maxscale_zoom7;
		&minscale_zoom7;
		<Filter>([waterway] = 'canal') and (([CEMT] = 'IV') or ([CEMT] = 'Va') or ([CEMT] = 'Vb') or ([CEMT] = 'VIa') or ([CEMT] = 'VIb') or ([CEMT] = 'VIb') or ([CEMT] = 'VII') or ([motorboat] = 'yes'))</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="1.1" stroke-opacity="0.6" />
		<LinePatternSymbolizer file="symbols-topo/canal.png" />
	</Rule>
	<Rule>
		&maxscale_zoom8;
		&minscale_zoom16;
		<Filter>([waterway] = 'canal') and (([CEMT] = 'IV') or ([CEMT] = 'Va') or ([CEMT] = 'Vb') or ([CEMT] = 'VIa') or ([CEMT] = 'VIb') or ([CEMT] = 'VIb') or ([CEMT] = 'VII') or ([motorboat] = 'yes'))</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="2" stroke-opacity="0.6" />
		<LinePatternSymbolizer file="symbols-topo/canal_2.png" />
	</Rule>
	
<!-- less important canals -->
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom16;
		<Filter>([waterway] = 'canal') and ([tunnel] != 'yes') and not (([CEMT] = 'IV') or ([CEMT] = 'Va') or ([CEMT] = 'Vb') or ([CEMT] = 'VIa') or ([CEMT] = 'VIb') or ([CEMT] = 'VIb') or ([CEMT] = 'VII') or ([motorboat] = 'yes'))</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="1.8" stroke-opacity="0.5" />
		<LinePatternSymbolizer file="symbols-topo/canal.png" />
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom16;
		<Filter>([waterway] = 'canal') and ([tunnel] = 'yes') and not (([CEMT] = 'IV') or ([CEMT] = 'Va') or ([CEMT] = 'Vb') or ([CEMT] = 'VIa') or ([CEMT] = 'VIb') or ([CEMT] = 'VIb') or ([CEMT] = 'VII') or ([motorboat] = 'yes'))</Filter>
		<LineSymbolizer stroke="#1d4dff" stroke-width="1.2" stroke-opacity="0.5" stroke-dasharray="5,5" />
		<!--LinePatternSymbolizer file="symbols-topo/canal.png" /-->
	</Rule>

</Style>
